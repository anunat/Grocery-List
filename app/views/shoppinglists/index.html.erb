<div class="page-header">
 <h3><strong>Shopping List</strong> <a href="/users/editlist" class="btn btn-lg btn-primary">Back</a></h3>
</div>

<div class="row">
  <div class="col-md-8">
    <table class="table table-striped table-hover">
      <tr>
        <th>Created</th>
        <th>Actions</th>
      </tr>

      <% @shoppinglists.reverse_each do |shoppinglist| %>
      <tr>
        <td><%= time_ago_in_words(shoppinglist.created_at) %> ago</td>
        <td>
          <a href="/shoppinglists/<%= shoppinglist.id %>" class="btn btn-primary"><i class="fa fa-search"></i></a>
          <a href="/delete_shoppinglist/<%= shoppinglist.id %>" class="btn btn-danger" rel="nofollow"><i class="fa fa-trash"></i></a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
